{
  "name": "scone-dictionary",
  "displayName": "scone_dictionary",
  "description": "Syntax highlighting of input files of SCONE Monte Carlo code",
  "version": "0.0.1",
  "publisher": "Cambridge Nuclear",
  "license": "MIT",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "scone-dict",
      "aliases": ["SCONE", "scone-dict"],
      "extensions": [".sin"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "scone-dict",
      "scopeName": "source.scone",
      "path": "./syntaxes/scone-dict.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CambridgeNuclear/vscode-scone.git"
  }
}
